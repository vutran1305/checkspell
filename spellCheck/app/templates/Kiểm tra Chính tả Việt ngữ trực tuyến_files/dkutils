(function(n){n.getProperties=function(n){var i="",t;for(t in n)i+=t+":"+n[t]+"\r\n";return i};n.createDate=function(n){return new Date(Date.parse(n))};String.prototype.format=function(){for(var t=this,i,n=0;n<arguments.length;n++)i=new RegExp("\\{"+n+"\\}","gm"),t=t.replace(i,arguments[n]);return t};Date.prototype.dayOfWeek=function(){return["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"][this.getDay()]}})(window.dkUtils=window.dkUtils||{},jQuery)